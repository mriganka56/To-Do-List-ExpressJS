<%- include('header'); -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <%for(var i=0;i<newAdd.length;i++){%>
    <div class="item">
        <input type="checkbox">
        <p> <%= newAdd[i] %> </p>
    </div>
    <% } %>
    <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="Add New" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
    </div>

<%- include('footer'); -%>
